{
	"name": "ceo-business.net",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://ceoatbusiness.net",
	"dependencies": {
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-photo-gallery": "^8.0.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^5.0.1"
	},
	"scripts": {
		"analyze": "source-map-explorer build/static/js/main.*",
		"build-style": "postcss src/styles/index.css -o src/index.css --env production",
		"watch-style": "postcss src/styles/index.css -o src/index.css -w",
		"eject": "react-scripts eject",
		"start": "npm-run-all -p watch-style start-app",
		"build": "npm-run-all build-style build-app copy-404",
		"start-app": "react-scripts start",
		"build-app": "react-scripts build",
		"copy-404": "cp build/index.html build/404.html",
		"test": "react-scripts test --env=jsdom",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^1.3.0",
		"autoprefixer": "^9.8.6",
		"gh-pages": "^2.2.0",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^6.1.3",
		"postcss-import": "^12.0.1",
		"tailwindcss": "^1.9.6"
	}
}
